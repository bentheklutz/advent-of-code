    "version": "0.2.0",
    "configurations": [
        {
            "name": "0501",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/2023/05/out/0501",
            "args": [
                "-e"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/2023/05",
            "environment": [],
            "externalConsole": false,
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "lldb"
            },
            "linux": {
                "MIMode": "gdb"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
